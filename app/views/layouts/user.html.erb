<!DOCTYPE html>
<html lang="<%= I18n.locale.to_s %>">
  <head>
    <%= render 'my/shared/head' %>
  </head>
  <body>
    <div class="gg-top-nav">
      <%= render partial: 'layouts/navigation' %>
    </div>
    <div class="menu-wrapper">
      <div>
        <%= render 'my/shared/navigation' %>
      </div>
      <div class="main">
        <div class="container">
          <div class="leftside">
            <%= render partial: "my/shared/menus" %>
          </div>
          <div class="rightside">
            <%= yield %>
          </div>

        </div>
      </div>
    </div>
    <%#= render 'layouts/footer' %>
  </body>
</html>
