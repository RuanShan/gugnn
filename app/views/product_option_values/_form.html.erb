<%= simple_form_for(@product_option_value) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :category_id %>
    <%= f.input :product_id %>
    <%= f.input :category_option_id %>
    <%= f.input :title %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
